cp "templates/day.rs" "src/year_${year}/day_07.rs"
cp "templates/day.rs" "src/year_${year}/day_08.rs"
cp "templates/day.rs" "src/year_${year}/day_09.rs"
cp "templates/day.rs" "src/year_${year}/day_10.rs"
cp "templates/day.rs" "src/year_${year}/day_11.rs"
cp "templates/day.rs" "src/year_${year}/day_12.rs"
cp "templates/day.rs" "src/year_${year}/day_13.rs"
cp "templates/day.rs" "src/year_${year}/day_14.rs"
cp "templates/day.rs" "src/year_${year}/day_15.rs"
cp "templates/day.rs" "src/year_${year}/day_16.rs"
cp "templates/day.rs" "src/year_${year}/day_17.rs"
cp "templates/day.rs" "src/year_${year}/day_18.rs"
cp "templates/day.rs" "src/year_${year}/day_19.rs"
cp "templates/day.rs" "src/year_${year}/day_20.rs"
